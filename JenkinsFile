pipeline{
    

    agent any
    environment {
        NEW_VERSION = '1.3.0'
    }
    stages {
        stage(build) {
            steps {
                echo "Hello World from Build form vs"
                echo "Build form build ${NEW_VERSION}"
                }
        }

        stage(test) {
            steps {
              echo "Hello World from tests"
                }
        }

        stage(deploy) {
            steps {
                echo "Hello World from deploy"
                }
        }
    }

}